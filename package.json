{
  "name": "world-of-code",
  "version": "0.0.1",
  "description": "Turn your youtube channel into a learning platform.",
  "engines": {
    "node": ">= 7.0.0"
  },
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "lint": "eslint ./ --ignore-path .gitignore",
    "postinstall": "touch secrets.js",
    "seed": "node scripts/seed.js",
    "start": "node server",
    "start-dev": "yarn run build-client-watch & yarn run start-server",
    "start-server": "NODE_ENV='development' nodemon server -e html,js,scss --ignore public",
    "test": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/world-of-code-test' mocha ./server/**/*.spec.js ./server/**/**/*.spec.js ./client/**/*.spec.js ./test/**/*.spec.js --compilers js:babel-register --exit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheUniverseofCoding/world-of-code"
  },
  "keywords": [
    "chrome",
    "extension",
    "react",
    "redux",
    "hot reload",
    "webpack",
    "boilerplate"
  ],
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "author": "Andrew Ziegler, Kyle Uehlein, Mariel Werner, Veekas Shrivastava",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.16.1",
    "bootstrap": "4.0.0-beta.2",
    "brace": "^0.11.0",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "connect-session-sequelize": "^4.1.0",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "history": "^4.6.3",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "passport-youtube-v3": "^2.1.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "rc-drawer": "^0.4.11",
    "react": "^16.0.0",
    "react-chrome-redux": "^1.4.0",
    "react-dom": "^16.0.0",
    "react-frame-component": "^2.0.0",
    "react-redux": "^5.0.2",
    "react-resizable-component": "^1.1.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^4.8.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sequelize": "^4.3.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.10.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.2.0",
    "chromedriver": "^2.33.2",
    "css-loader": "^0.25.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^3.9.1",
    "eslint-config-fullstack": "^3.0.0",
    "eslint-plugin-react": "^6.5.0",
    "file-loader": "^1.1.5",
    "jquery": "^3.2.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.1",
    "nodemon": "^1.12.1",
    "react-test-renderer": "^16.1.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.1",
    "supertest": "^3.0.0",
    "webpack": "^3.8.1",
    "webpack-livereload-plugin": "^1.0.0"
  }
}
